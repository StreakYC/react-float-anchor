{
  "name": "react-float-anchor",
  "version": "3.4.0",
  "description": "React component for positioning an element aligned to another",
  "main": "dist/src/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "rimraf dist && babel -s true -d dist/src/ src/ -x .ts,.tsx --ignore '**/*.test.ts,**/*.test.tsx' && tsc",
    "example-build": "cross-env NODE_ENV=development browserify --extension=.ts --extension=.tsx -t [ babelify --extensions .ts,.tsx ] -do example/bundle.js example/main.tsx",
    "example-watch": "cross-env NODE_ENV=development watchify --extension=.ts --extension=.tsx -v -t [ babelify --extensions .ts,.tsx ] -do example/bundle.js example/main.tsx",
    "example-build-prod": "cross-env NODE_ENV=production browserify --extension=.ts --extension=.tsx -t [ babelify --extensions .ts,.tsx ] -do example/bundle.js example/main.tsx",
    "test": "yarn run lint && flow check && jest && tsc --noEmit --emitDeclarationOnly false",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StreakYC/react-float-anchor.git"
  },
  "keywords": [
    "react",
    "react-component",
    "menu",
    "submenu"
  ],
  "files": [
    "dist",
    "index.js.flow"
  ],
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StreakYC/react-float-anchor/issues"
  },
  "homepage": "https://github.com/StreakYC/react-float-anchor#readme",
  "jest": {
    "modulePathIgnorePatterns": ["<rootDir>/dist/"]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.0.0",
    "@types/jest": "^28.1.6",
    "@types/kefir": "^3.8.7",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/sinon": "^10.0.13",
    "@types/sinon-test": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "^28.1.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.184.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.0",
    "sinon": "^14.0.0",
    "sinon-test": "^3.0.0",
    "typescript": "^4.1.5",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "contain-by-screen": "^1.0.4",
    "event-listener-with-options": "^1.0.2",
    "kefir": "^3.5.1",
    "kefir-bus": "^2.2.0",
    "lodash": "^4.6.1",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}

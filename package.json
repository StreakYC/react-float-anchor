{
  "name": "react-float-anchor",
  "version": "1.0.0",
  "description": "React component for positioning an element aligned to another",
  "main": "js/index.js",
  "scripts": {
    "prepublish": "babel -s inline -d js/ src/ && flow-copy-source -v src js",
    "example-build": "NODE_ENV=development browserify -t babelify -do example/bundle.js example/main.js",
    "example-watch": "NODE_ENV=development-hmr watchify -v -p browserify-hmr -t babelify -do example/bundle.js example/main.js",
    "example-build-prod": "NODE_ENV=production browserify -t babelify -do example/bundle.js example/main.js",
    "test": "npm run lint && mocha",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StreakYC/react-float-anchor.git"
  },
  "keywords": [
    "react",
    "react-component",
    "menu",
    "submenu"
  ],
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StreakYC/react-float-anchor/issues"
  },
  "homepage": "https://github.com/StreakYC/react-float-anchor#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.6.5",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "flow-copy-source": "^1.0.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "sinon": "^1.17.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "contain-by-screen": "^1.0.4",
    "kefir": "^3.2.1",
    "kefir-bus": "^2.1.0",
    "lodash": "^4.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
